{
  "name": "cochan",
  "version": "0.5.1",
  "description": "Channel communication primitive",
  "author": "Sam Kozin <sam.kozin@gmail.com>",
  "scripts": {
    "clean": "rm -rf ./lib ./es6",
    "prepublish": "npm run clean && npm run build-es6 && npm run build-es5",
    "build-es5": "BABEL_ENV=production babel --presets es2015 --out-dir ./lib ./src",
    "build-es6": "BABEL_ENV=production babel --out-dir ./es6 ./src",
    "watch": "npm run clean && BABEL_ENV=dev babel --watch --presets es2015 --out-dir ./lib ./src",
    "test": "echo 'Sorry, no tests yet' && exit 1"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [ "unassert", "transform-remove-console" ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skozin/cochan.git"
  },
  "keywords": [
    "chan",
    "channel",
    "coroutine",
    "csp",
    "pubsub",
    "pub-sub",
    "async",
    "await",
    "generator",
    "co",
    "go",
    "goroutine",
    "concurrency",
    "flow",
    "control",
    "promise",
    "golang",
    "es6",
    "es2015",
    "es7"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skozin/cochan/issues"
  },
  "homepage": "https://github.com/skozin/cochan",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-remove-console": "^6.5.0",
    "babel-plugin-unassert": "^2.0.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "co": "^4.6.0",
    "readable-stream": "^2.0.5"
  },
  "files": [
    "es6",
    "lib"
  ],
  "main": "lib/index.js"
}
